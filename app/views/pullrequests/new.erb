
<% provide(:title, 'Add P/R') %>
<h1>Add P/R</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @pullrequest do |f| %>
        <div class="form-group">
          <%= f.label :ipserver, "Ip Server" %>
          <%= f.text_field :ipserver, :class=>"form-control"%>
        </div>
        <div class="form-group">
          <%= f.label :linkpullrequest, "Link Pull Request" %>
          <%= f.text_field :linkpullrequest, :class=>"form-control"%>
        </div>

        <div class="form-group">
          <%= f.label :note, "Note" %>
          <%= f.text_area :note, :class=>"form-control" %>
        </div>


        <%= f.submit "Add P/R", class: "btn btn-primary" %>

    <% end %>
  </div>
</div>
<script>
  $(document).ready(function() {
    $('#pullrequest_note').summernote();
  });
</script>

